{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "lib": ["es2020"],
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "ES2017",
    "sourceMap": true,
    "declaration": true,
    "importHelpers": true,
    "strictNullChecks": true
  }
}





